{
  "name": "artevida-sql-agent",
  "version": "1.0.0",
  "description": "Agente conversacional que convierte lenguaje natural a consultas SQL MySQL",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:100": "cross-env RUN_100_PROMPTS=true vitest --run test/chat_prompts_100.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "tsx src/migrate.ts",
    "seed": "tsx src/seed.ts",
    "run:prompts": "tsx scripts/run_prompts.ts",
    "run:test-questions": "tsx scripts/run_test_questions.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "setup": "tsx scripts/setup.ts"
  },
  "keywords": [
    "sql",
    "nlp",
    "mysql",
    "express",
    "typescript",
    "conversational-ai"
  ],
  "author": "ArteVida Cultural",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.3",
    "node-sql-parser": "^4.17.0",
    "openai": "^4.104.0",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "node-fetch": "^3.3.2",
    "pino-pretty": "^13.1.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
